/* Import nib */
@import 'nib'

html, body
  width 100%
  height 100%

body
  overflow hidden
  &.active
    background #FF00FF

#drop
  position absolute
  top 0
  left 0
  width 100%
  height 100%
  #heart
    position fixed
    width 110px
    height 100px
    background-size 100% auto
    left 50%
    top 50%
    margin-left -55px
    margin-top -50px
    background-image url('data:image/png;base64,')

  &.dragging
    background-color #FF00FF
    #heart
      background-position 0px -100px
      pointer-events none

  &.loading
    background-color #FF00FF
    #heart
      pointer-events none